{
    "name": "tempest/intl",
    "description": "A component for working with internationalization.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "doctrine/inflector": "^2.0",
        "symfony/yaml": "^7.3",
        "tempest/core": "1.0.3",
        "tempest/container": "1.0.3",
        "tempest/support": "1.0.3"
    },
    "suggest": {
        "tempest/datetime": "In order to use the `datetime` function"
    },
    "autoload": {
        "files": [
            "src/functions.php",
            "src/Number/functions.php"
        ],
        "psr-4": {
            "Tempest\\Intl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Intl\\Tests\\": "tests"
        }
    }
}
